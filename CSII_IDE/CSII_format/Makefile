###############################################################
#

###############################################################
# Variables
CPP  = clang++
OPTS = -g -Wall -W -Wunused -Wuninitialized -Wshadow -std=c++11



###############################################################


wp:  main.o 
	$(CPP) $(OPTS) main.cpp -o wp





###############################################################
clean:
	rm -f *.o
	rm -f wp
